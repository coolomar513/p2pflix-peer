import json
import requests

def add_file(filename):
    print(filename)
    j = json()
    j = {
        "name": "<file name>",
        "full_hash": "<hash of full file>",
        "chunks": [
            {
                "id": 42069,
                "name": "my chunk filename",
                "hash": "s654fd65as4df6as4d65f"
            }
        ],
        "guid": "<client's guid>"
    }
    r = requests.post('http://localhost:42069/add_file', j)
